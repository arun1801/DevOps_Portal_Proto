node("docker_hybris")
{
 stage("Hybris Docker containers Deployment | DEV Machine")
 {
 def mysql_container = 'docker run -d -v /home/osboxes/hybris/mysql/lib:/var/lib/mysql --name hybris.c.mysql arunsingh1801/hybris-mysql:latest'
 sh "ssh -o StrictHostKeyChecking=no osboxes@10.0.2.4 mkdir -p /home/osboxes/hybris/mysql/lib"
 sh "ssh -o StrictHostKeyChecking=no osboxes@10.0.2.4 ${mysql_container}"
}
}
